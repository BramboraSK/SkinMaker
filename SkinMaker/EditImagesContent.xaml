<UserControl x:Class="SkinMaker.EditImagesContent"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkinMaker"
             mc:Ignorable="d" 
             d:DesignHeight="570" d:DesignWidth="1080">

    <Grid>
        <ListBox x:Name="ImageList" Background="{x:Null}" Foreground="White" Margin="0,0,540,0" SelectionChanged="ImageList_SelectionChanged"/>
        <Image x:Name="Preview" HorizontalAlignment="Left" Height="270" Margin="675,62,0,0" VerticalAlignment="Top" Width="270"/>
    </Grid>

</UserControl>
