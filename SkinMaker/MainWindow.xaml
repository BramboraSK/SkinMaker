<Window x:Name="MainWindow1" x:Class="SkinMaker.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SkinMaker"
        mc:Ignorable="d"
        Title="Skin Maker" Height="720" Width="1280" FontSize="20">
    <Grid>
        <Button x:Name="NewSkin" Content="Button" HorizontalAlignment="Left" Margin="100,344,0,0" VerticalAlignment="Top" Height="60" Width="225" Click="NewSkin_Click"/>
        <Button x:Name="OpenSkin" Content="Button" HorizontalAlignment="Left" Margin="350,344,0,0" VerticalAlignment="Top" Height="60" Width="250"/>
        <Button x:Name="Options" Content="Button" HorizontalAlignment="Left" Margin="100,444,0,0" VerticalAlignment="Top" Height="60" Width="500"/>
        <Button x:Name="Exit" Content="Button" HorizontalAlignment="Left" Margin="100,544,0,0" VerticalAlignment="Top" Height="60" Width="500"/>
    </Grid>
</Window>
