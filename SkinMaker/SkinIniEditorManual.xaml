<UserControl x:Class="SkinMaker.SkinIniEditorManual"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SkinMaker"
             mc:Ignorable="d" 
             Loaded="OnLoad"
             d:DesignHeight="634" d:DesignWidth="1280"
             BorderThickness="0,1,0,0"
             BorderBrush="#FFABADB3">
    <Grid>
        <TextBox x:Name="EditorTextBox" HorizontalAlignment="Center" TextChanged="TextBox_TextChanged" TextWrapping="NoWrap" Text="" VerticalAlignment="Center" Height="554" Width="1200" Foreground="White" Background="{x:Null}"/>

    </Grid>
</UserControl>
